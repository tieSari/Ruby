<h3>Most active raters</h3>

<table class="table table-hover">
  <thead>
  <tr>
    <th>Rater</th>
    <th>Count</th>
  </tr>
  </thead>

  <tbody>
  <% users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%=user.ratings.count %></td>
        <td></td>
      </tr>
  <% end %>
  </tbody>
</table>