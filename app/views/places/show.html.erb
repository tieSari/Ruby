<p id="notice"><%= notice %></p>

<% if @place %>
    <p><h2><%=@place.name%></h2></p>
    <p><%=@place.street%></p>
    <p><%=@place.zip%> 	<%=@place.city%></p>

    <div id="theMap">
      <iframe width="600" height="350" frameborder="3" scrolling="no" marginheight="0" marginwidth="0" src="<%=@place.blogmap%>"></iframe><br><small><a href="<%=@place.blogmap%>" style="color:#fff;text-align:left">View Larger Map</a></small>
    </div>

<% end %>
<%= link_to 'Back', places_path %>